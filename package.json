{
  "name": "@primatif/hyena",
  "version": "1.1.0",
  "description": "Hyena Design System - A comprehensive design system for AI-driven canvas content generation with atomic components and centralized design tokens",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tokens": {
      "import": "./dist/tokens.js",
      "require": "./dist/tokens.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ci": "vitest --run --coverage",
    "preview": "vite preview",
    "pdf:style-guide": "node scripts/generate-pdf.js",
    "pdf:service-sheet": "node scripts/generate-service-sheet.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run lint && npm run test:ci && npm run build:lib"
  },
  "keywords": [
    "hyena",
    "design-system",
    "react",
    "components",
    "ui",
    "atomic-design",
    "design-tokens",
    "ai-canvas",
    "generative-design"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Primatif/Hyena.git"
  },
  "homepage": "https://github.com/Primatif/Hyena#readme",
  "bugs": {
    "url": "https://github.com/Primatif/Hyena/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "image-js": "^0.37.0",
    "react-router-dom": "^7.6.2",
    "react-syntax-highlighter": "^15.6.1",
    "skmeans": "^0.11.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/addon-docs": "^9.0.14",
    "@storybook/addon-vitest": "^9.0.14",
    "@storybook/react-vite": "^9.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.14",
    "globals": "^16.0.0",
    "playwright": "^1.53.1",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "puppeteer": "^24.10.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.14",
    "tailwindcss": "^4.1.10",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
